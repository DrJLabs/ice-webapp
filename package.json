{
  "name": "ice-webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "codex:verify": "node --version && npm --version && echo 'Codex environment OK'",
    "coverage:upload": "export CODACY_PROJECT_TOKEN=$CODACY_PROJECT_TOKEN && bash <(curl -Ls https://coverage.codacy.com/get.sh) report -r ./coverage/lcov.info"
  },
  "dependencies": {
    "next": "^15.1.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "clsx": "^2.1.0",
    "tailwind-merge": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "typescript": "^5.7.2",
    "eslint": "^9.18.0",
    "eslint-config-next": "^15.1.3",
    "tailwindcss": "^4.1.8",
    "postcss": "^8.4.38",
    "autoprefixer": "^10.4.19",
    "vitest": "^3.2.2",
    "@vitest/coverage-v8": "^3.2.2",
    "playwright": "^1.49.0",
    "esbuild": "^0.25.0",
    "jsdom": "^24.0.0"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
