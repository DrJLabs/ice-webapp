{
  "name": "ice-webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "codex:verify": "node --version && npm --version && echo 'Codex environment OK'",
    "coverage:upload": "export CODACY_PROJECT_TOKEN=$CODACY_PROJECT_TOKEN && bash <(curl -Ls https://coverage.codacy.com/get.sh) report -r ./coverage/lcov.info",
    "codacy:quality-gates": "node scripts/codacy-quality-gates.js",
    "codacy:commit-quality-gates": "node scripts/codacy-commit-quality-gates.js",
    "codacy:setup-all": "pnpm run codacy:quality-gates && pnpm run codacy:commit-quality-gates",
    "prepare": "husky"
  },
  "dependencies": {
    "clsx": "^2.1.0",
    "next": "^15.3.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwind-merge": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.30",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.1",
    "@vitest/coverage-v8": "^3.2.2",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.0",
    "autoprefixer": "^10.4.21",
    "dotenv": "^16.5.0",
    "esbuild": "^0.25.5",
    "eslint": "^9.28.0",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "eslint-config-next": "^15.3.3",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "playwright": "^1.52.0",
    "postcss": "^8.5.4",
    "tailwindcss": "^4.1.8",
    "typescript": "^5.8.3",
    "vitest": "^3.2.2"
  },
  "engines": {
    "node": ">=20.19.1"
  }
}
